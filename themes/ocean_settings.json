{
  "music_folder": "sounds/music",
  "action_button_text_color": "#0fffd1",
  "action_button_background_color": "#072c33",
  "action_button_click_color": "#00bcd4",
  "button_active_color": "#00a896",
  "button_inactive_color": "#014f43",
  "auto_test_text_color": "#e0ffff",
  "sound_text_color": "#e0ffff",
  "music_test_text_color": "#e0ffff",
  "interval_text_color": "#008A8A",
  "interval_background_color": "#b2dfdb",
  "colors_list": ["#00ffff", "#40e0d0", "#7fffd4", "#20b2aa", "#008b8b", "#5f9ea0"],
  "background_color": "#002f36",
  "text_color": "#00ffff",
  "line_color": "#00ced1",

  "frame_background_color": "#003f47",
  "title_text_color": "#1debb7",
  "output_text_color": "#c8f4f9",
  "root_background_color": "#004d4d",

  "legend": {
    "text_color": "#00ffff",
    "enabled": true,
    "ncol": 2,
    "frameon": false
  },
  "colorbar": {
    "label": "Speed (Mbps)",
    "text_color": "#00ffff"
  },
  "grid": {
    "enabled": false,
    "color": "#66ffff",
    "linestyle": "-",
    "linewidth": 0.5
  },
  "scatter": {
    "edgecolor": "#e0ffff",
    "linewidth": 1.5,
    "marker": "x",
    "size": 40
  },
  "average_lines": {
    "enabled": true,
    "download": {
      "color": "#00ffff",
      "linestyle": "--",
      "linewidth": 1.2,
      "label_template": "Avg Download ({:.2f} Mbps)"
    },
    "upload": {
      "color": "#40e0d0",
      "linestyle": "--",
      "linewidth": 1.2,
      "label_template": "Avg Upload ({:.2f} Mbps)"
    }
  }
}
